{
  "compilerOptions": {
    "declaration": true,
		"module": "CommonJS",
    "strict": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "lib": ["dom", "es5", "es6", "scripthost"],
    "target": "es5",
    "rootDir": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "dist"]
}